  <body>
    <!--navbar-->

    <nav class="navbar">
      <div class="logo">
        <img id="logo_pic" src="/assets/logo.png" />
        <h3 id="name">AGREEcrop</h3>
      </div>
      <div class="namelogo">
        <p style="letter-spacing: 5px">WELCOME <%= userr.usname %></p>
      </div>
      <div class="profile">
        <img
          src="/assets/profile.png"
          class="main_profile"
          id="main_profile_id"
        />
        <p class="prof1">PROFILE</p>
        <div class="editview">
          <img
            src="/assets/eprofile.png"
            class="edit_profile"
            id="edit_profile_id"
          />
          <img
            src="/assets/vprofile.jpeg"
            class="view_profile"
            id="view_profile_id"
          />
        </div>
      </div>
    </nav>

    <!--second-->

    <div class="front">
      <div class="selection">
        <div class="home" id="home">
          <form action="/hom" method="post">
            <button id="home_btn">HOME</button>
          </form>
        </div>
        <div class="crops" id="crops">
          <form action="/crp" method="post">
            <button id="crop_btn">CROPS</button>
