              <details class="custom-select">
                <summary class="radios">
                  <input
                    type="radio"
                    name="season"
                    id="default"
                    title=">"
                    checked
                  />
                  <input type="radio" name="item" id="season1" title="KHARIF" value="kharif"/>
                  <input type="radio" name="item" id="season2" title="ZAID" value="zaid"/>
                  <input type="radio" name="item" id="season3" title="RABI" value="rabi"/>
